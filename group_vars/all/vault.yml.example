# =============================================================================
# Ansible Vault - Sensitive Data Template
# =============================================================================
# Create actual vault file with:
#   ansible-vault create group_vars/all/vault.yml
#
# Edit with:
#   ansible-vault edit group_vars/all/vault.yml
# =============================================================================

# WiFi Networks (example structure)
# wifi_networks:
#   - ssid: "HomeNetwork"
#     password: "secret_password"
#     priority: 1
#   - ssid: "WorkNetwork" 
#     password: "work_secret"
#     priority: 2

# SSH keys (if needed)
# ssh_private_key: |
#   -----BEGIN OPENSSH PRIVATE KEY-----
#   ...
#   -----END OPENSSH PRIVATE KEY-----

# API tokens
# github_token: "ghp_xxxx"
# telegram_api_id: "12345"
# telegram_api_hash: "abc123"

# Encryption passwords (for backup purposes)
# luks_backup_passphrase: "backup_passphrase"
